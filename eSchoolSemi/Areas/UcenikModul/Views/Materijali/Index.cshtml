@model MaterijalIndexVM
@{
    ViewData["Title"] = "Index";
}



<table class="table table-striped">
    <tr>
        <td><h3>Nastavni materijali</h3></td>
        <td><h3>Skini materijal</h3></td>

    </tr>
    @foreach (var x in Model.Rows)
    {
    <tr>
        <td>
            <h1>@x.Naziv iz @x.Predmet</h1>
            <br><small>Materijal postavio: @x.Nastavnik, @x.DatumObjave </small>

        </td>
       
        <td>
            <a class="btn btn-success" href="@x.FilePath" download>Skini</a>
        </td>

    </tr>


    }

</table>
