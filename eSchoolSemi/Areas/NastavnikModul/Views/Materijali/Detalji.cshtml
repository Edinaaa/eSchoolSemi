@model eSchoolSemi.Web.Areas.NastavnikModul.ViewModels.MaterijalDetaljVM
@{
    ViewData["Title"] = "Detalji";
    Layout = "~/Areas/NastavnikModul/Views/Shared/_Layout.cshtml";
}
<h1>Materijali</h1>
<div class="container-fluid">
    <div class="row">
        <div class="col-sm-12">
            <h3>@Model.Naziv</h3>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12" style="border:solid thin; width:100%; height:200px">
            <text>@Html.Raw(Model.Sadrzaj)</text>
        </div>
    </div>
</div>

